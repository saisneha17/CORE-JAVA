package com.dl.one;

public class Eg6 {

	public static void main(String[] args) {
		
   //for loop
	int i = 0;
	for (; i < 5; i++) {
		System.out.println("For Loop: " + i);
	}
	
	int j = 0;
	for (System.out.println("Sai Chandana"),System.out.println("Sweety"),System.out.println("Sonu"); j < 5; j++) {
		System.out.println("For Loop: " + j);
	}
	for (int a = 0, b = 0; a < 5 && b < 5; a++, b++) {
		System.out.println(a + " " +b);
	}
	for (int j2 = 0; ; j2++) {
		System.out.println(j2);
	    }

	}

}
