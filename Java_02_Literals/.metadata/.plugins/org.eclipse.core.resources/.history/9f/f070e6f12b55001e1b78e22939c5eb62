package com.dl.one;

public class Eg6 {

	public static void main(String[] args) {
		
		int a[] = {10, 20, 30, 40, 50, 60, 70, 80, 90 };
		System.out.println(a.length);
		
		for (int i = 0; i < a.length; i++) {
			System.out.println(a[i]);
		}
		
		System.out.println();
		
		for (int i = 3; i < 5; i++) {
			System.out.println(a[i]);
		}
		
		System.out.println();
		
		for (int i : a) {
			System.out.println(i);
		}
		System.out.println();
		
		for (int i = 0; i <= 8; i++) {
			System.out.println(a[i]);
			
		}
		
		System.out.println("--");
		
		for (int i = 0; ;i++) {
			System.out.println(a[i]);
			
		}
		

	}

}
