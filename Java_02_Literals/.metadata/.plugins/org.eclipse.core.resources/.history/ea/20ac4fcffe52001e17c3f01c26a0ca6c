//		contactNo = contactNo;
//		address = address;
//	}

//	public CreateAccount(String uName, String mail, String pword, long cNo, String add) {
//	userName = uName;
//	email = mail;
//	password = pword;
//	contactNo = cNo;
//	address = add;
//  }

	//instance method
	public void display() {
		System.out.println(userName);
		System.out.println(email);
		System.out.println(password);
		System.out.println(contactNo);
		System.out.println(address);
	}
	public static void main(String[] args) {
		
		CreateAccount account = new CreateAccount("Sai chandra","saichandra@gmail.com","admin@1234",9491472779L,"Hyd");
		account.display();
	}
	}
